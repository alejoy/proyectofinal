<h1>Productos</h1>

<table id="main">
  <tr>
    <th>Titulo</th>
    <th>Descripcion</th>
    <th>Codigo</th>
    <th>Precio</th>
    <th>Stock</th>
    <th>Imagen</th>
  </tr>
  {{#each students}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.code}}</td>
      <td>{{this.price}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.thumbnails}}</td>
    </tr>
  {{/each}}
</table>